@model IEnumerable<Microsoft.AspNet.Identity.IUser>

@{
    ViewBag.Title = "Пользователи";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Список пользователей</h2>
<table class="table">
    <tr>
        <th>Логин</th>
        <th>ФИО</th>
        <th></th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => user.UserName)</td>
            <td>
                @Html.DisplayFor(modelItem => user.LastName)
                @Html.DisplayFor(modelItem => user.FirstName)
                @Html.DisplayFor(modelItem => user.Patronymic)
            </td>
            <td>
                <form action="@Html.ActionLink("Изменить", "Edit", new { id = user.Id })" method="post">
                    <button type="submit" class="btn btn-sm btn-danger"> Удалить</button>
                </form>
                <form action="@Html.ActionLink("Удалить", "Delete", new { id = user.Id })" method="post">
                    <button type="submit" class="btn btn-sm btn-danger"> Удалить</button>
                </form>
            </td>
        </tr>
    }
</table>