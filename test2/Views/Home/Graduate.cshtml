@using University.Models
@model IEnumerable<Graduate>

@{
    ViewBag.Title = "Студенты";
}
<br /><br />
      
          <table class="table">
              <tr>
                  <th>@Html.DisplayNameFor(model => model.Id)</th>
                  <th>Фамилия</th>
                  <th>Имя</th>
                  <th>Отчество</th>
                  <th>id Группы</th>
                  <th>Город</th>
                  <th>id Компании</th>
                  <th>Лекции</th>
                  <th>Лабораторные</th>
                  <th>Академическая <br />степень</th>
              </tr>
              @foreach (var item in Model)
              {
                  <tr>
                      <td>@Html.DisplayFor(modelItem => item.Id)</td>
                      <td>@Html.DisplayFor(modelItem => item.LastName)</td>
                      <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
                      <td>@Html.DisplayFor(modelItem => item.Patronymic)</td>
                      <td>@Html.DisplayFor(modelItem => item.GroupId)</td>
                      <td>@Html.DisplayFor(modelItem => item.CurrentCity)</td>
                      <td>@Html.DisplayFor(modelItem => item.CompanyId)</td>
                      <td>@Html.DisplayFor(modelItem => item.DisciplineLecture)</td>
                      <td>@Html.DisplayFor(modelItem => item.DisciplineLaboratoryWorks)</td>
                      <td>@Html.DisplayFor(modelItem => item.AcademicDegreeId)</td>
                  </tr>
              }
          </table>
      